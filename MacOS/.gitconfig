[credential]
  helper = osxkeychain
[user]
  name = Ryan Miller
  email = rmiller@synergybis.com
[grep]
  lineNumber = true
[alias]
  aa = add --all
  amend = commit --amend
  br = branch
  cam = commit -a -m
  co = commit
  ch = checkout
  cl = clean -df
  di = diff
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  pr = remote prune origin
  rh = reset --hard
  st = status
  unstage = reset --soft
